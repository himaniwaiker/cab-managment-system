.r_contextualstore .c-pivot { margin-bottom: 48px; }

.m-smode-warning { padding-top: 64px; }

.m-smode-warning > * { margin-top: 5px; }

#gift-action-modal-dialog.c-dialog > [role="presentation"] { background: rgba(0, 0, 0, 0.9); }

.d_xbox #gift-action-modal-dialog.c-dialog button.glyph-cancel { display: none; }

#gift-action-modal-dialog.c-dialog [role="dialog"] { width: 100%; }

@media screen and (min-width: 540px) {
  #gift-action-modal-dialog.c-dialog [role="dialog"] { width: 458px; }
}

#gift-action-modal-dialog.c-dialog [role="document"] { min-height: 488px; }

.d_xbox #gift-action-modal-dialog.c-dialog [role="document"] { min-height: 420px; }

#gift-action-modal-dialog.c-dialog [role="document"] .c-image img, #gift-action-modal-dialog.c-dialog [role="document"] .c-image img { max-height: 170px; }

#gift-action-modal-dialog.c-dialog [role="document"] .m-product-placement-item { margin: 0px; }

#gift-action-modal-dialog.c-dialog [role="document"] .m-product-placement-item.f-size-small { width: auto; }

#gift-action-modal-dialog.c-dialog [role="document"] .c-select { max-width: none; }

#gift-action-modal-dialog.c-dialog [role="document"] .c-select::after { content: none; }

.d_xbox #gift-action-modal-dialog.c-dialog.gift-dialog-layout-short [role="document"] { min-height: 300px; }

.shoppingCartGlyph { font-size: 65px; margin-right: 20px; opacity: 0.6; }

.cartReminderButtonGroup { text-align: right; }

.cartReminderMessageGroup { margin-top: 8px; }

.newsletter-email-field { margin-top: 12px !important; }

.newsletter-email-field-column { margin-right: 6px; }

.newsletter-email-feedback { color: rgb(255, 0, 0); }

.newsletter-email-form { padding-bottom: 5px; }

.sfw-dialog { height: auto !important; }

.email-newsletter-dialog-title { padding-bottom: 8px !important; }

.c-uhff { background: rgb(242, 242, 242); margin-top: 48px; }

.c-uhff .c-modal-link { cursor: pointer; }

.c-uhff-nav { margin: 0px auto; max-width: calc(1600px + 10%); padding: 0px 5%; }

.c-uhff-nav::before, .c-uhff-nav::after { content: " "; display: table; }

.c-uhff-nav::after { clear: both; }

.c-uhff-nav .c-heading-4 { font-size: 15px; line-height: 20px; }

.c-uhff-nav .c-heading-4, .c-uhff-nav a.c-uhff-link { color: rgb(97, 97, 97); word-break: break-word; }

.c-uhff-nav-group { display: block; float: left; min-height: 1px; vertical-align: text-top; padding: 0px 12px; width: 16.6667%; zoom: 1; }

.c-uhff-nav-group ul.c-list { font-size: 11px; line-height: 16px; margin-top: 0px; }

.c-uhff-nav-group:first-child { padding-left: 0px; }

.c-uhff-base { background: rgb(242, 242, 242); margin: 0px auto; max-width: calc(1600px + 10%); padding: 30px 5% 16px; }

.c-uhff-base::before, .c-uhff-base::after { content: " "; display: table; }

.c-uhff-base::after { clear: both; }

.c-uhff-base > a.c-uhff-link.c-uhff-lang-selector { font-size: 11px; line-height: 16px; display: inline-block; padding-right: 24px; color: rgb(97, 97, 97); float: left; white-space: nowrap; }

.c-uhff-base > a.c-uhff-link.c-uhff-lang-selector::before { font-size: 20px; line-height: 24px; margin-right: 10px; vertical-align: middle; }

.c-uhff-base ul.c-list { font-size: 11px; line-height: 16px; float: right; margin: 3px 0px; color: rgb(97, 97, 97); }

.c-uhff-base ul.c-list li { padding: 0px 24px 4px 0px; display: inline-block; }

@media only screen and (max-width: 1083px) {
  .c-uhff-nav { padding: 0px 12px; }
  .c-uhff-nav-row::before, .c-uhff-nav-row::after { content: " "; display: table; }
  .c-uhff-nav-row::after { clear: both; }
  .c-uhff-nav-group { width: 33.3333%; }
  .c-uhff-nav-group:first-child { padding-left: 12px; }
  .c-uhff-base { display: flex; flex-wrap: wrap; padding: 30px 24px 16px; }
  .c-uhff-base > a.c-uhff-link { margin-right: auto; }
}

@media only screen and (max-width: 767px) {
  .c-uhff-base > a.c-uhff-link { width: 100%; }
  .c-uhff-base > a.c-uhff-link + ul.c-list { width: 100%; }
  .c-uhff-base > a.c-uhff-link, .c-uhff-base > a.c-uhff-link.c-glyph { float: none; }
  .c-uhff-base ul.c-list { width: 100%; float: none; text-align: left; margin-top: 12px; }
  .c-uhff-base ul.c-list li { padding: 6px 4px 0px 6px; }
}

@media only screen and (max-width: 539px) {
  .c-uhff-nav-row { width: 100%; float: none; }
  .c-uhff-base ul.c-list { float: left; }
  .c-uhff-base ul.c-list li { padding: 6px 10px 0px 0px; }
  .c-uhff-nav-group { width: 100%; }
}

@supports (-ms-ime-align:auto) {
  a.c-uhff-link { display: inline-block; }
}

a.c-uhff-link:focus { outline: dashed 1px; outline-offset: 3px; }

a.c-uhff-link:focus, a.c-uhff-link:hover { color: rgba(0, 0, 0, 0.9); text-decoration: underline; }

a.c-uhff-link:active { background: none; color: rgba(0, 0, 0, 0.8); }
